{
  "outputDirectory": "dist",
  "devCommand": "vite --port 3000",
  "cleanUrls": true,
  "buildCommand": "pnpm run build",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}