{
  "name": "shadcnui",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "powershell -File start-dev.ps1",
    "stop": "powershell -File stop-dev.ps1",
    "check": "powershell -File check-servers.ps1",
    "restart": "powershell -Command \"& .\\stop-dev.ps1; & .\\start-dev.ps1\"",
    "build": "vite build",
    "vercel-build": "vite build",
    "lint": "eslint --quiet ./src",
    "preview": "vite preview",
    "deploy": "vercel --prod",
    "fix-ids": "node scripts/fix-ids.js",
    "fix-ids:check": "node scripts/fix-ids.js --check",
    "fix-ids:fix": "node scripts/fix-ids.js --fix",
    "backup": "node scripts/backup-supabase.js",
    "backup:verbose": "node scripts/backup-supabase.js --verbose",
    "backup:list": "powershell -File scripts/list-backups.ps1",
    "backup:list:detailed": "powershell -File scripts/list-backups.ps1 -Detailed",
    "backup:compare": "powershell -File scripts/list-backups.ps1 -Compare",
    "restore": "node scripts/restore-supabase.js",
    "restore:latest": "node scripts/restore-supabase.js --latest",
    "restore:dry": "node scripts/restore-supabase.js --latest --dry-run",
    "history:all": "node scripts/list-history.js all",
    "history:list": "node scripts/list-history.js list",
    "history:view": "node scripts/list-history.js view",
    "history:export": "node scripts/list-history.js export"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.80.0",
    "@tiptap/extension-color": "^3.10.4",
    "@tiptap/extension-highlight": "^3.10.4",
    "@tiptap/extension-image": "^3.10.4",
    "@tiptap/extension-link": "^3.10.4",
    "@tiptap/extension-text-align": "^3.10.4",
    "@tiptap/extension-text-style": "^3.10.4",
    "@tiptap/extension-underline": "^3.10.4",
    "@tiptap/react": "^3.10.4",
    "@tiptap/starter-kit": "^3.10.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.553.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react-swc": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  }
